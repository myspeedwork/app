{
    "name": "speedwork/app",
    "type": "application",
    "description": "A Speedwork Application",
    "license": "MIT",
    "authors": [{
        "name": "sankar",
        "email": "sankar.suda@gmail.com"
    }],
    "require": {
        "php": ">=5.6",
        "speedwork/installer": "^1.0",
        "speedwork/container": "^1.0",
        "speedwork/core": "^1.0",
        "speedwork/util": "^1.0",
        "speedwork/config": "^1.0",
        "speedwork/view": "^1.0",
        "speedwork/provider": "^1.0",
        "speedwork/filesystem": "^1.0",
        "speedwork/console": "^1.0",
        "speedwork/cache": "^1.0",
        "speedwork/helpers": "^1.0",
        "speedwork/database": "^1.0",
        "symfony/event-dispatcher": "^3.1",
        "symfony/http-foundation": "^3.1",
        "symfony/http-kernel": "^3.1",
        "filp/whoops": "^2.1",
        "phpmailer/phpmailer": "^5.2",
        "smarty/smarty": "^3.1",
        "twig/twig": "^1.24",
        "monolog/monolog": "^1.21",
        "kriswallsmith/assetic": "^1.3",
        "natxet/CssMin": "^3.0",
        "mrclay/minify": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "satooshi/php-coveralls": "2.*"
    },
    "suggest": {
        "willdurand/geocoder": "~v3 for Geo location",
        "geoip2/geoip2": "~v2 for Geo location",
        "piwik/device-detector": "3.4.2 For auto detect device"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php ./console key:generate"
        ]
    },
    "autoload": {
        "psr-4": {
            "System\\": "system/"
        },
        "classmap" : ["AppServiceProvider.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
