{
    "name": "speedwork/app",
    "type": "application",
    "description": "A Speedwork Application",
    "license": "MIT",
    "authors": [{
        "name": "sankar",
        "email": "sankar.suda@gmail.com"
    }],
    "repositories" : [
        {
            "type" : "vcs",
            "url" : "D:\\xampp\\htdocs\\laravel-spark-google2fa"
        }
    ],
    "require": {
        "php": ">=5.6",
        "speedwork/framework" : "dev-master",
        "turbophp/speedwork": "dev-master",
        "filp/whoops": "^2.1",
        "smarty/smarty": "^3.1",
        "twig/twig": "^1.24",
        "eusebiu/laravel-spark": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "satooshi/php-coveralls": "2.*"
    },
    "suggest": {
        "willdurand/geocoder": "~v3 for Geo location",
        "geoip2/geoip2": "~v2 for Geo location",
        "piwik/device-detector": "3.4.2 For auto detect device"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php ./console key:generate"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
